package in.api.entity;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import lombok.Data;


@Data
@Entity
public class Ticket 
{
	@Id
	String ticketNumber;
	
	Integer aadhaarCard;
    String name;
    String phoneNumber;
    String start;
    String end;
    String status;
    String price;
}
